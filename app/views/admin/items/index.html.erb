<h2>商品一覧</h2>
<div class="row">
	<div class="col-xs-9">
		<table class="table">
			<thead>
				<tr>
					<th scope="col">商品ID</th>
					<th scope="col">商品名</th>
					<th scope="col">税抜価格</th>
					<th scope="col">ジャンル</th>
					<th scope="col">ステータス</th>
				</tr>
			</thead>
			<tbody>
				<% @items.each do |i| %>
				<tr>
					<td><%= i.id %></td>
					<td><%= i.name %></td>
					<td><%= i.price %></td>
					<td><%= i.ganre_id %></td>
					<td><%= i.is_active %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>
</div>